{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"esModuleInterop": true,
		"rootDir": "./src/ts",
		"outDir": "./src/js",
		"sourceMap": false,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"noImplicitAny": true,
		"allowImportingTsExtensions": true,
	}
}